<div class="container-fluid">
  {{#if messages.success}}
    <div role="alert" class="alert alert-success">
      {{#each messages.success}}
        <div>{{msg}}</div>
      {{/each}}
    </div>
  {{/if}}
  {{#if messages.error}}
    <div role="alert" class="alert alert-danger">
      {{#each messages.error}}
        <div>{{msg}}</div>
      {{/each}}
    </div>
  {{/if}}
  {{#if messages.info}}
    <div role="alert" class="alert alert-info">
      {{#each messages.info}}
        <div>{{msg}}</div>
      {{/each}}
    </div>
  {{/if}}
  <div class="row home-landing">
    <div>
      <div class="content">
          <h1>Powering your digital currency with its own debit card and credit card.</h1>
          <br>
          <a href="/about" class="btn btn-primary my-btn">&nbsp;&nbsp;&nbsp;DISCOVER HOW&nbsp;&nbsp;&nbsp;</a>
      </div>
    </div>
  </div>
</div>
